{
  "name": "dbgate",
  "version": "7.0.0-alpha.1",
  "private": true,
  "author": "Jan Prochazka <jenasoft.database@gmail.com>",
  "description": "Opensource database administration tool",
  "dependencies": {
    "@aws-sdk/rds-signer": "^3.665.0",
    "@azure/cosmos": "*",
    "@clickhouse/client": "*",
    "@duckdb/node-api": "*",
    "@mongosh/browser-runtime-electron": "*",
    "@mongosh/service-provider-node-driver": "*",
    "activedirectory2": "^2.1.0",
    "axios": "^1.13.2",
    "better-sqlite3": "*",
    "bson": "*",
    "electron-log": "^4.4.1",
    "electron-updater": "^6.3.4",
    "ioredis": "*",
    "jsonwebtoken": "^8.5.1",
    "libsql": "*",
    "lodash.clonedeepwith": "^4.5.0",
    "mongodb": "*",
    "mongodb-client-encryption": "*",
    "mongodb-old": "*",
    "msnodesqlv8": "*",
    "mysql2": "*",
    "node-redis-dump2": "*",
    "oracledb": "*",
    "patch-package": "^6.4.7",
    "pg": "*",
    "pg-copy-streams": "*",
    "ssh2": "^1.16.0",
    "tedious": "*",
    "wkx": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgate/dbgate.git"
  },
  "build": {
    "artifactName": "dbgate-${version}-${os}_${arch}.${ext}",
    "appId": "org.dbgate",
    "productName": "DbGate",
    "asarUnpack": "**/*.node",
    "directories": {
      "output": "dist",
      "app": "."
    },
    "mac": {
      "category": "database",
      "icon": "icon512-mac.png",
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "publish": [
        {
          "provider": "github",
          "owner": "dbgate",
          "repo": "dbgate"
        }
      ],
      "target": {
        "target": "default",
        "arch": [
          "universal",
          "x64",
          "arm64"
        ]
      },
      "notarize": true
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        "tar.gz"
      ],
      "icon": "icons/",
      "category": "Development",
      "synopsis": "Database manager for SQL Server, MySQL, PostgreSQL, MongoDB and SQLite",
      "publish": [
        {
          "provider": "github",
          "owner": "dbgate",
          "repo": "dbgate"
        }
      ]
    },
    "appImage": {
      "license": "./LICENSE",
      "category": "Development"
    },
    "snap": {
      "publish": [
        "github",
        "snapStore"
      ],
      "environment": {
        "ELECTRON_SNAP": "true"
      }
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.ico",
      "publish": [
        {
          "provider": "github",
          "owner": "dbgate",
          "repo": "dbgate"
        }
      ]
    },
    "files": [
      "packages",
      "src",
      "icon.png",
      "node_modules/**/*",
      "!node_modules/**/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/**/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/**/*.d.ts",
      "!node_modules/**/*.map",
      "!node_modules/**/*.config.js",
      "!node_modules/**/jest.config.js",
      "!node_modules/**/webpack.config.js",
      "!node_modules/**/rollup.config.js",
      "!node_modules/**/vite.config.js",
      "!node_modules/**/tsconfig.json",
      "!node_modules/**/.eslintrc*",
      "!node_modules/**/.prettierrc*",
      "!node_modules/dbgate-*/**/*.config.js",
      "!node_modules/dbgate-*/**/jest.config.js",
      "!node_modules/dbgate-*/**/tsconfig.json",
      "!node_modules/dbgate-*/**/src/**",
      "!node_modules/cpu-features/build/**"
    ],
    "beforePack": "./clean-configs.js",
    "asar": true
  },
  "homepage": "./",
  "scripts": {
    "start": "cross-env ELECTRON_START_URL=http://localhost:5001 DEVMODE=1 electron .",
    "start:local": "cross-env electron .",
    "dist": "electron-builder --publish never",
    "build": "cd ../packages/api && yarn build && cd ../web && yarn build && cd ../../app && yarn predist && yarn dist",
    "build:local": "cd ../packages/api && yarn build && cd ../web && yarn build && cd ../../app && yarn predist",
    "postinstall": "yarn rebuild && patch-package",
    "rebuild": "electron-builder install-app-deps",
    "clean:configs": "node clean-configs.js",
    "predist": "copyfiles ../packages/api/dist/* packages && copyfiles \"../packages/web/public/*\" packages && copyfiles \"../packages/web/public/**/*\" packages && copyfiles --up 3 \"../plugins/dist/**/*\" packages/plugins && yarn clean:configs"
  },
  "main": "src/electron.js",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "cross-env": "^6.0.3",
    "electron": "38.6.0",
    "electron-builder": "25.1.8"
  }
}
