{
  "name": "dbmanager-pro",
  "version": "1.0.0",
  "description": "A powerful database management tool like Navicat",
  "author": "Your Name",
  "license": "MIT",
  "main": "dist/electron/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "start": "concurrently \"vite --port 3000\" \"wait-on http://localhost:3000 && npm run build:main && cross-env NODE_ENV=development electron .\"",
    "dev:main": "cross-env NODE_ENV=development tsc -p tsconfig.electron.json --watch",
    "dev:renderer": "cross-env NODE_ENV=development vite --config vite.config.ts",
    "build": "npm run build:main && npm run build:renderer && npm run copy:dbgate",
    "build:main": "tsc -p tsconfig.electron.json",
    "build:renderer": "vite build --config vite.config.ts",
    "copy:dbgate": "node scripts/copy-dbgate-files.js",
    "postinstall": "electron-builder install-app-deps",
    "build:app": "npm run build && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "preview": "electron .",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/list": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@fullcalendar/vue3": "^6.1.20",
    "@guolao/vue-monaco-editor": "^1.1.3",
    "@paddlejs-models/ocr": "1.2.0",
    "@types/ws": "^8.18.1",
    "@vue/shared": "^3.5.26",
    "activedirectory2": "^2.2.0",
    "archiver": "^7.0.1",
    "async-lock": "^1.4.1",
    "axios": "^1.13.4",
    "body-parser": "^2.2.2",
    "byline": "^5.0.0",
    "compare-versions": "^6.1.1",
    "cors": "^2.8.6",
    "crypto-js": "^4.1.1",
    "echarts": "^5.4.3",
    "element-plus": "^2.13.0",
    "express": "^5.2.1",
    "express-basic-auth": "^1.2.1",
    "express-fileupload": "^1.5.2",
    "external-sorting": "^1.3.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.3.3",
    "fs-reverse": "^0.0.3",
    "get-port": "^7.1.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.3.2",
    "is-electron": "^2.2.2",
    "js-base64": "^3.7.8",
    "js-yaml": "^4.1.1",
    "json-stable-stringify": "^1.3.0",
    "jsonwebtoken": "^9.0.3",
    "line-reader": "^0.4.0",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.21",
    "lunar-javascript": "^1.7.7",
    "moment": "^2.30.1",
    "mongodb": "^6.3.0",
    "mysql2": "^3.6.5",
    "ncp": "^2.0.0",
    "node-cron": "^4.2.1",
    "on-finished": "^2.4.1",
    "onnxruntime-web": "^1.24.0-dev.20251116-b39e144322",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "pinomin": "^1.0.5",
    "portfinder": "^1.0.38",
    "rimraf": "^6.1.2",
    "semver": "^7.7.3",
    "simple-encryptor": "^4.0.0",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.17.0",
    "stream-json": "^1.9.1",
    "tar": "^7.5.7",
    "tedious": "^16.2.0",
    "tesseract.js": "^7.0.0",
    "utif": "^3.1.0",
    "vue": "^3.4.3",
    "vue-router": "^4.6.4",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0",
    "ws": "^8.19.0",
    "xlsx": "^0.18.5",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.10.4",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.10.9",
    "@types/sqlite3": "^3.1.8",
    "@types/tedious": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.55.0",
    "less": "^4.5.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-svgr": "^4.5.0",
    "vue-tsc": "^1.8.27",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.dbmanager.pro",
    "productName": "DBManager Pro",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist/**/*",
      "dbgate-master/packages/api/dist/**/*",
      "dbgate-master/app/packages/**/*",
      "dbgate-master/plugins/**/*",
      "node_modules/**/*",
      "package.json",
      "!dbgate-master/**/node_modules/**/*",
      "!dbgate-master/**/*.map"
    ],
    "asarUnpack": [
      "**/*.node",
      "dbgate-master/packages/api/dist/**/*",
      "dbgate-master/app/packages/**/*",
      "dbgate-master/plugins/**/*"
    ],
    "beforeBuild": null,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        "zip"
      ],
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "icon": "build/icon.png"
    }
  }
}
